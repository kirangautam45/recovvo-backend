{
  "name": "recovo-backend",
  "version": "0.1.0",
  "description": "Recovo Backend",
  "main": "index.js",
  "engines": {
    "node": ">= 10.16.0",
    "yarn": ">= 1.15.2"
  },
  "scripts": {
    "start": "NODE_ENV=production node build/main.js",
    "local": "backpack",
    "clean": "rimraf build",
    "transpile": "backpack build",
    "build": "run-s clean lint:fix transpile",
    "send:mail": "ts-node scripts/mail-test",
    "load:fake": "NODE_ENV=test ts-node scripts/fake-loader",
    "test:admin": "NODE_ENV=test yarn migrate-admin && NODE_ENV=test yarn seed-admin && NODE_ENV=test jest --forceExit --detectOpenHandles --maxWorkers=1 --testPathPattern=/src/admin",
    "test:client": "NODE_ENV=test yarn migrate-client && NODE_ENV=test yarn seed-client && NODE_ENV=test jest --forceExit --detectOpenHandles --maxWorkers=1 --testPathPattern=/src/client",
    "migrate": "yarn migrate-admin && yarn tenant:setup",
    "seed": "yarn seed-admin && yarn seed-client",
    "seed-admin": "knex seed:run --knexfile=knexfile-admin.ts --verbose",
    "migrate-admin": "knex migrate:latest --knexfile=knexfile-admin.ts --verbose",
    "rollback-admin": "knex migrate:rollback --knexfile=knexfile-admin.ts --verbose",
    "make:seeder:common": "knex seed:make --knexfile=knexfile-admin.ts -x ts",
    "make:migration:common": "knex migrate:make --knexfile=knexfile-admin.ts -x ts",
    "seed-client": "knex seed:run --knexfile=knexfile-client.ts --verbose",
    "migrate-client": "knex migrate:latest --knexfile=knexfile-client.ts --verbose",
    "rollback-client": "knex migrate:rollback --knexfile=knexfile-client.ts --verbose",
    "make:seeder:tenant": "knex seed:make --knexfile=knexfile-client.ts -x ts",
    "make:migration:tenant": "knex migrate:make --knexfile=knexfile-client.ts -x ts",
    "lint": "eslint {src,scripts,test}/**/*.{ts,json} --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix {src,scripts,test}/**/*.{ts,json} --no-error-on-unmatched-pattern",
    "tenant:setup": "./client-schema.sh setup",
    "tenant:setup-client": "./client-schema.sh setup-client",
    "tenant:create-admin": "ts-node ./src/core/cli/adminTenantGenerator.ts create",
    "tenant:update-admin": "ts-node ./src/core/cli/adminTenantGenerator.ts update",
    "tenant:seed-local": "./client-schema.sh seed-client",
    "tenant:migrate-table-local": "./client-schema.sh migrate",
    "tenant:rollback-table-local": "./client-schema.sh rollback-migrate",
    "tenant:create-schema-local": "ts-node ./src/core/cli/tenantSchemaGenerator.ts create",
    "tenant:drop-schema-local": "ts-node ./src/core/cli/tenantSchemaGenerator.ts drop",
    "tenant:invite": "ts-node ./src/core/cli/sendEmail.ts",
    "tenant:encode-subject-snippet": "ts-node ./src/core/cli/subjectSnippetEncoder.ts",
    "tenant:move-credential": "ts-node ./src/core/cli/moveCredentialToSecretManager.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn build"
    }
  },
  "lint-staged": {
    "*.{ts,json}": [
      "eslint --fix {src,scripts,test}/**/*.{ts,json} --no-error-on-unmatched-pattern"
    ]
  },
  "private": true,
  "license": "MIT",
  "keywords": [
    "api",
    "es6",
    "node",
    "express",
    "javascript",
    "typescript"
  ],
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@leapfrogtechnology/async-store": "^1.2.0",
    "@leapfrogtechnology/db-model": "1.0.0-beta.3",
    "aws-sdk": "^2.711.0",
    "axios": "0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.2",
    "bookshelf": "^1.2.0",
    "bookshelf-case-converter-plugin": "^2.0.0",
    "camelcase-keys": "6.2.2",
    "chalk": "^4.1.0",
    "clear": "0.1.0",
    "commander": "5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "debug": "4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "figlet": "1.5.0",
    "google-auth-library": "^6.0.2",
    "helmet": "^3.23.0",
    "http-status-codes": "^1.4.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.3",
    "knex": "^0.21.1",
    "latest": "^0.2.0",
    "lodash": "^4.17.15",
    "mixpanel": "^0.13.0",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.8",
    "nodemailer-markdown": "^1.0.3",
    "papaparse": "^5.2.0",
    "path": "0.12.7",
    "pg": "^8.2.1",
    "ramda": "^0.27.0",
    "snakecase-keys": "^3.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "to-snake-case": "1.0.0",
    "tslib": "^2.1.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bookshelf": "^1.1.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-serve-static-core": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.0",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.13",
    "@types/nodemailer": "^6.4.0",
    "@types/papaparse": "^5.0.4",
    "@types/ramda": "^0.27.3",
    "@types/supertest": "^2.0.9",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/to-snake-case": "^1.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "backpack-core": "^0.8.4",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^5.0.0",
    "husky": "^4.2.5",
    "inversify": "^5.0.1",
    "jest": "^26.0.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
